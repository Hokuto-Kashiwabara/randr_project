<table>
    <thead>
        <tr>
            <th class="col-sm-2">id</th>
            <th class="col-sm-10">title</th>
        </tr>
    </thead>

    <tbody>
        <!-- リスト -->
        <% var i = 0; %>
        <% docs.forEach( function(doc) { %>
            <tr>
                <td><%- doc.id -%></td>
                <td><%- doc.title -%></td>
                <input type="hidden" id="id" value="<%- doc.body %>">
            </tr>
        <% i++; %>
        <% }) %>
    </tbody>
</table>