<table>
    <thead>
        <tr>
            <th class="col-sm-2">id</th>
            <th class="col-sm-10">title</th>
        </tr>
    </thead>

    <tbody>
        <!-- リスト -->
        <% var i = 0; %>
        <% params.params.forEach(function() { %>
            <tr>
                <td><%- app.app.nvl(params.params[i].params.id) -%></td>
                <td><%- app.app.nvl(params.params[i].params.title) -%></td>
                <input type="hidden" id="id" value="<%- params.params[i].params.body %>">
            </tr>
        <% i++; %>
        <% }) %>
    </tbody>
</table>